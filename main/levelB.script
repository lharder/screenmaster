function init(self)
	pprint( "Level B loaded!"  )
end



function update(self, dt)
	pprint( "Level B updating..." )
end


function on_message(self, message_id, message, sender)
	pprimt( "Level B received message: " .. message_id )
end


function on_input(self, action_id, action)
	pprimt( "Level B receiving input: " .. action_id )
end
